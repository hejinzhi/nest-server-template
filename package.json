{
  "name": "vue3-admin-server",
  "version": "nest-server-template",
  "private": true,
  "description": "vue3-admin-server",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "serve": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prestart": "rimraf dist && npm run build",
    "setup": "pm2 deploy ecosystem.json production setup",
    "upload": "pm2 deploy ecosystem.json production"
  },
  "dependencies": {
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/microservices": "^6.5.3",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.11.11",
    "@nestjs/platform-fastify": "^6.5.3",
    "@nestjs/platform-ws": "^6.5.3",
    "@nestjs/swagger": "^3.1.0",
    "@nestjs/typeorm": "^6.1.3",
    "@nestjs/websockets": "^6.5.3",
    "amqplib": "^0.5.5",
    "buffer-image-size": "^0.6.4",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1",
    "co-wechat-api": "^3.10.0",
    "commander": "^3.0.1",
    "compression": "^1.7.4",
    "cos-nodejs-sdk-v5": "^2.5.12",
    "death": "^1.1.0",
    "express-rate-limit": "^5.0.0",
    "fastify-rate-limit": "^2.2.0",
    "fastify-swagger": "^2.4.0",
    "hash.js": "^1.1.7",
    "helmet": "^3.20.0",
    "http-proxy-agent": "^2.1.0",
    "https-proxy-agent": "^3.0.1",
    "imagemin": "^7.0.1",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
    "isbn": "^0.4.0",
    "moment": "^2.24.0",
    "mysql": "^2.17.1",
    "nest-schedule": "^0.6.3",
    "nestjs-amqp": "^0.1.3",
    "nestjs-config": "^1.4.3",
    "nestjs-console": "^1.2.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.12.0",
    "promise-status-async": "^1.1.9",
    "qcloud-cos-sts": "^3.0.2",
    "reflect-metadata": "^0.1.12",
    "resize-img": "^2.0.0",
    "rimraf": "^2.6.2",
    "rxjs": "^6.5.2",
    "simple-isbn": "^1.1.5",
    "swagger-ui-express": "^4.0.7",
    "tencentcloud-sdk-nodejs": "3.0.198",
    "typeorm": "0.2.25",
    "uuid": "^3.3.2",
    "xlsx": "^0.15.1",
    "xml2js": "^0.4.22"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.0.0",
    "@types/amqplib": "^0.5.13",
    "@types/express": "^4.16.0",
    "@types/imagemin": "^7.0.0",
    "@types/imagemin-pngquant": "^7.0.0",
    "@types/imagemin-mozjpeg": "^8.0.0",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.18",
    "@types/passport-jwt": "^3.0.1",
    "@types/supertest": "^2.0.7",
    "@types/uuid": "^3.4.5",
    "@types/ws": "^6.0.1",
    "@types/xml2js": "^0.4.5",
    "concurrently": "^4.1.0",
    "husky": "^3.0.4",
    "jest": "^23.6.0",
    "lint-staged": "^9.2.5",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "supertest": "^3.4.1",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typescript": "^3.7.2",
    "wait-on": "^3.2.0",
    "ws": "^7.1.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
